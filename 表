# 关联爬虫表&进件表
jiea.p_loan t1
openline.credit_app_customer t2 -- 用户ID, ID_Num
openline.credit_app_crawler_app_no t3
t1.user_id = t2.id => t2.apply_no
t2.apply_no = t3.apply_no => t3.crawler_apply_no
# 创建表（分区表）

